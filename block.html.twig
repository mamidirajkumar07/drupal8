{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
    'clearfix',
  ]
%}
<section{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {% if label %}
    <h2{{ title_attributes.addClass('block-title') }}>{{ "Block Training" }} _ {{ label }}</h2>
  {% endif %}
  {{ title_suffix }}
{{ label }}
  {% block content %}
    {{ content }}
  {% endblock %}
</section>

